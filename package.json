  "license": "ISC",
  "dependencies": {
    "http-errors": "^1.5.1",
    "lodash": "^4.17.4",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feliperohdee/smallorange-acl"
  },
  "homepage": "https://smallorange.co"
}
